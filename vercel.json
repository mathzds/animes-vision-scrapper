{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/episodes",
      "dest": "/api/episodes"
    },
    {
      "src": "/api/home",
      "dest": "/api/home"
    },
    {
      "src": "/api/search",
      "dest": "/api/search"
    },
    {
      "src": "/api/video",
      "dest": "/api/video"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}
