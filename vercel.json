{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/episodes",
      "dest": "src/index.js", 
      "methods": ["GET"]
    },
    {
      "src": "/api/home",
      "dest": "src/index.js", 
      "methods": ["GET"]
    },
    {
      "src": "/api/search",
      "dest": "src/index.js", 
      "methods": ["GET"]
    },
    {
      "src": "/api/video",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js" 
    }
  ]
}
