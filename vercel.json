{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    { 
      "src": "/api/episodes", 
      "dest": "index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
